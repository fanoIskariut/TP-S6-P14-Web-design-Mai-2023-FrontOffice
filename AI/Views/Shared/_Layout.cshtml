
<!-- OutputCache duration: 3600, location: Server, varyByParam: "*" -->
<!doctype html>
<html class="no-js"  lang="en">

	<head>
		<!-- META DATA -->
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

		<!--font-family-->
		

		<!-- TITLE OF SITE -->
		<title>@ViewData["Title"] - AI</title>

		<!-- favicon img -->
		<link rel="shortcut icon" type="image/icon" href="~/logo/favicon.png"/>

		<!--font-awesome.min.css-->
		<link rel="stylesheet" href="~/css/font-awesome.min.css " asp-append-version="true"/>

		<!--animate.css-->
		<link rel="stylesheet" href="~/css/animate.css" asp-append-version="true" />

		<!--hover.css-->
		<link rel="stylesheet" href="~/css/hover-min.css" asp-append-version="true">

		<!--datepicker.css-->
		<link rel="stylesheet"  href="~/css/datepicker.css" asp-append-version="true" >

		<!--owl.carousel.css-->
        <link rel="stylesheet" href="~/css/owl.carousel.min.css" asp-append-version="true">
		<link rel="stylesheet" href="~/css/owl.theme.default.min.css" asp-append-version="true"/>

		<!-- range css-->
        <link rel="stylesheet" href="~/css/jquery-ui.min.css"  />

		<!--bootstrap.min.css-->
		<link rel="stylesheet" href="~/css/bootstrap.min.css" />

		<!-- bootsnav -->
		<link rel="stylesheet" href="~/css/bootsnav.css"/>

		<!--style.css-->
		<link rel="stylesheet" href="~/css/style.css" asp-append-version="true"/>

		<!--responsive.css-->
		<link rel="stylesheet" href="~/css/responsive.css" asp-append-version="true" />

		<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
		<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->

		<!--[if lt IE 9]>
		
		<![endif]-->

	</head>
<body>
<!-- main-menu Start -->
		<header class="top-area">
			<div class="header-area">
				<div class="container">
					<div class="row">
						<div class="col-sm-2">
							<div class="logo">
								<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">A<span>I</span></a>
							</div><!-- /.logo-->
						</div><!-- /.col-->
						<div class="col-sm-10">
							<div class="main-menu">
							
								<!-- Brand and toggle get grouped for better mobile display -->
								<div class="navbar-header">
									<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
										<i class="fa fa-bars"></i>
									</button><!-- / button-->
								</div><!-- /.navbar-header-->
								<div class="collapse navbar-collapse">		  
									<ul class="nav navbar-nav navbar-right">
										<li class="smooth-menu"> 
											@using (Html.BeginForm("Index", "Home")){<button type="submit" class="btn btn-primary">Home</button>}
										</li>
										
										<li class="smooth-menu"> 
										  @using (Html.BeginForm("Index", "AIType")){<button type="submit" class="btn btn-primary">Top</button>}
										</li>
									</ul>
								</div><!-- /.navbar-collapse -->
							</div><!-- /.main-menu-->
						</div><!-- /.col-->
					</div><!-- /.row -->
					<div class="home-border"></div><!-- /.home-border-->
				</div><!-- /.container-->
			</div><!-- /.header-area -->

		</header><!-- /.top-area-->
		<!-- main-menu End -->
<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer  class="footer-copyright">
			<div class="container">
	

						<div class="col-sm-3">
							<div class="single-footer-item">
								<div class="footer-logo">
									<a asp-area="" asp-controller="Home" asp-action="Index">
										A<span>I</span>
									</a>
									<p>
										best travel agency
									</p>
								</div>
							</div><!--/.single-footer-item-->
						</div><!--/.col-->
					<hr>
                    <div class="foot-icons ">
                    	<ul class="footer-social-links list-inline list-unstyled">
                            <li><a href="#" target="_blank" class="foot-icon-bg-1"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#" target="_blank" class="foot-icon-bg-2"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#" target="_blank" class="foot-icon-bg-3"><i class="fa fa-instagram"></i></a></li>
                        </ul>
                        
    
                    </div><!--/.foot-icons-->
                    <div id="scroll-Top">
                    	<i class="fa fa-angle-double-up return-to-top" id="scroll-top" data-toggle="tooltip" data-placement="top" title="" data-original-title="Back to Top" aria-hidden="true"></i>
                    </div><!--/.scroll-Top-->
                </div><!-- /.container-->
</footer>

<script src="~/js/jquery.js"></script>
		<!-- Include all compiled plugins (below), or include individual files as needed -->

		
		


		<!--bootstrap.min.js-->
		<script  src="~/js/bootstrap.min.js"></script>

		<!-- bootsnav js -->
		<script src="~/js/bootsnav.js"></script>

		<!-- jquery.filterizr.min.js -->
		<script src="~/js/jquery.filterizr.min.js"></script>

	

		<!--jquery-ui.min.js-->
        <script src="~/js/jquery-ui.min.js"></script>

        <!-- counter js -->
		<script src="~/js/jquery.counterup.min.js"></script>
		<script src="~/js/waypoints.min.js" asp-append-version="true"></script>

		<!--owl.carousel.js-->
        <script  src="~/js/owl.carousel.min.js" asp-append-version="true"></script>

        <!-- jquery.sticky.js -->
		<script src="~/js/jquery.sticky.js"></script>

        <!--datepicker.js-->
        <script  src="~/js/datepicker.js" asp-append-version="true"></script>

		<!--Custom JS-->
		<script src="~/js/custom.js" asp-append-version="true"></script>
		@await RenderSectionAsync("Scripts", required: false)

	</body>

</html>